{
  "name": "pyrobot",
  "version": "0.0.2a",
  "private": true,
  "description": "Pyrobot",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node --inspect",
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "fix": "yarn run lint --fix",
    "lint": "eslint -c .eslintrc.js --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "precommit": "lint-staged"
  },
  "author": "Pyroarsonist <velikiydan@gmail.com>",
  "dependencies": {
    "async": "^2.2.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "copyfiles": "^2.0.0",
    "dotenv": "^6.0.0",
    "events": "^3.0.0",
    "express": "^4.16.3",
    "http": "^0.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "named-regexp": "^0.1.1",
    "node-telegram-bot-api": "^0.30.0",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-modern-regexp": "^0.0.5",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-es6": "^1.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint --fix",
      "git add --force"
    ]
  }
}
